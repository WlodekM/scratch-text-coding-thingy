{
  "tasks": {
    "run": "deno run -A ./main.ts project",
    "run-and-open": "deno run -A main.ts project & turbowarp-desktop project/project.sb3",
    "open": "turbowarp-desktop project/project.sb3"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1"
  },
  "lint": {
    "rules": {
      "exclude": ["no-explicit-any"]
    }
  },
  "fmt": {
    "options": {
      "useTabs": true
    }
  }
}
