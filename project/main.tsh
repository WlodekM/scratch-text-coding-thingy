#include <"blocks/js" "tw-blocks/blocks_vertical/control.js">
#include <"blocks/js" "tw-blocks/blocks_vertical/motion.js">
#include <"blocks/js" "tw-blocks/blocks_vertical/operators.js">

gf {
    // var a = 5
    // looks_say(a)
    var vy = 0
    motion_gotoxy(0, 180)
    control_forever() {
        vy = operator_subtract(vy, 1)
        if (operator_lt(motion_xposition(), operator_subtract(0,100))) {
            vy = 0
        }
        motion_changeyby(vy)
    }
}